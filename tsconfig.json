{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "typeRoots": [
      "node_modules/@types",
      "../../node_modules/@types"
    ],
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
